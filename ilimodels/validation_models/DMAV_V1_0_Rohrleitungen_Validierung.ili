    VIEW TOPIC Rohrleitungen_Validierung = 
    DEPENDS ON DMAV_Rohrleitungen_V1_0.Rohrleitungen;
 
        VIEW v_Leitungsobjekt
            PROJECTION OF DMAV_Rohrleitungen_V1_0.Rohrleitungen.Leitungsobjekt;
        =
            ALL OF Leitungsobjekt;

            !!@ ilivalid.msg="Hoehengeometrie darf nicht 0.0 sein";
            !!@ ilivalid.msg_de="Hoehengeometrie darf nicht 0.0 sein";
            !!@ ilivalid.msg_fr="GeometrieAltitude ne peut pas être égale à 0.0";
            !!@ ilivalid.msg_it="La geometria dell'altezza non deve essere uguale a 0.0";
            !! AGI SH: Failcase umgesetzt;
            MANDATORY CONSTRAINT CH092551: (
                Punktelement->Hoehengeometrie <> 0.0
            );

        END v_Leitungsobjekt;

        VIEW v_Messpunkt
            PROJECTION OF DMAV_Rohrleitungen_V1_0.Rohrleitungen.Messpunkt;
        =
            ALL OF Messpunkt;

            !!@ ilivalid.msg="Hoehengeometrie darf nicht 0.0 sein";
            !!@ ilivalid.msg_de="Hoehengeometrie darf nicht 0.0 sein";
            !!@ ilivalid.msg_fr="GeometrieAltitude ne peut pas être égale à 0.0";
            !!@ ilivalid.msg_it="La geometria dell'altezza non deve essere uguale a 0.0";
            MANDATORY CONSTRAINT CH101503: (
                Messpunkt->Hoehengeometrie <> 0.0
            );

        END v_Messpunkt; 

    END Rohrleitungen_Validierung;

END DMAV_V1_0_Rohrleitungen_Validierung.
